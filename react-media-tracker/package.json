{
  "name": "react-media-tracker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "-------------------- comment 1 --------------------": "Main scripts:",
    "android-dev": "npm run setup-env-android-dev && react-native run-android",
    "android-prod": "npm run setup-env-android-prod && react-native run-android --variant=release",
    "ios-dev": "npm run setup-env-ios-dev; react-native run-ios",
    "ios-dev-ipad": "npm run setup-env-ios-dev; react-native run-ios --simulator \"iPad (7th generation)\"",
    "ios-prod": "npm run setup-env-ios-prod; react-native run-ios --device --configuration Release",
    "-------------------- comment 2 --------------------": "Util scripts:",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "ios-clean": "cd ios && xcodebuild clean && cd ..",
    "ios-pod": "cd ios; pod install --repo-update; cd ..",
    "reset": "react-native start --reset-cache",
    "lint": "eslint 'app/**/*.ts' 'app/**/*.tsx'",
    "-------------------- comment 3 --------------------": "Script components:",
    "setup-env-android-dev": "npm run echo-env-dev && npm run cp-firebase-android-dev",
    "setup-env-android-prod": "npm run echo-env-prod && npm run cp-firebase-android-prod",
    "setup-env-ios-dev": "npm run echo-env-dev; npm run cp-firebase-ios-dev",
    "setup-env-ios-prod": "npm run echo-env-prod; npm run cp-firebase-ios-prod",
    "cp-firebase-android-dev": "shx cp \"./app/config/files/config-dev/google-services.json\" \"./android/app/\"",
    "cp-firebase-android-prod": "shx cp \"./app/config/files/config-prod/google-services.json\" \"./android/app/\"",
    "cp-firebase-ios-dev": "shx cp \"./app/config/files/config-dev/GoogleService-Info.plist\" \"./ios/\"",
    "cp-firebase-ios-prod": "shx cp \"./app/config/files/config-prod/GoogleService-Info.plist\" \"./ios/\"",
    "echo-env-dev": "shx echo \"MEDIA_TRACKER_APP_ENV=dev\" > .env",
    "echo-env-prod": "shx echo \"MEDIA_TRACKER_APP_ENV=prod\" > .env"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.19.1",
    "@react-native-community/datetimepicker": "7.4.1",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-firebase/app": "18.3.0",
    "@react-native-firebase/auth": "18.3.0",
    "@react-native-picker/picker": "2.4.10",
    "@react-navigation/drawer": "6.6.3",
    "@react-navigation/native": "6.1.7",
    "@react-navigation/stack": "6.3.17",
    "axios": "1.4.0",
    "class-transformer": "0.5.1",
    "class-transformer-validator": "0.9.1",
    "class-validator": "0.14.0",
    "date-fns": "2.30.0",
    "formik": "2.4.3",
    "i18n-js": "4.3.0",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "react-native-config": "1.5.1",
    "react-native-document-picker": "9.0.1",
    "react-native-flash-message": "0.4.1",
    "react-native-fs": "2.20.0",
    "react-native-gesture-handler": "2.12.1",
    "react-native-reanimated": "3.4.2",
    "react-native-safe-area-context": "4.7.1",
    "react-native-screens": "3.24.0",
    "react-redux": "8.1.2",
    "redux": "4.2.1",
    "redux-saga": "1.2.3",
    "string-strip-html": "8.5.0",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-decorators": "7.22.10",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "7.20.0",
    "@react-native-community/eslint-config": "3.2.0",
    "@react-native/eslint-config": "0.72.2",
    "@react-native/metro-config": "0.72.9",
    "@tsconfig/react-native": "3.0.0",
    "@types/i18n-js": "3.8.4",
    "@types/jest": "29.5.3",
    "@types/react-native": "0.72.2",
    "@types/react-redux": "7.1.25",
    "@types/react-test-renderer": "18.0.0",
    "@types/react": "18.0.24",
    "@types/redux": "3.6.31",
    "@types/string-strip-html": "5.0.1",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "babel-jest": "29.2.1",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jsdoc": "46.4.6",
    "eslint-plugin-react": "7.33.1",
    "eslint": "8.19.0",
    "jest": "29.2.1",
    "metro-react-native-babel-preset": "0.76.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "reflect-metadata": "0.1.13",
    "shx": "0.3.4",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "@types/react": "17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}

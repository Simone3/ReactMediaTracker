{
  "name": "react-media-tracker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android-dev": "npm run setup-env-android-dev && react-native run-android",
    "android-prod": "npm run setup-env-android-prod && react-native run-android --variant=release",
    "ios-dev": "npm run setup-env-ios-dev; react-native run-ios",
    "ios-dev-ipad": "npm run setup-env-ios-dev; react-native run-ios --simulator \"iPad (7th generation)\"",
    "ios-prod": "npm run setup-env-ios-prod; react-native run-ios --device --configuration Release",
    "setup-env-android-dev": "npm run echo-env-dev && npm run cp-firebase-android-dev",
    "setup-env-android-prod": "npm run echo-env-prod && npm run cp-firebase-android-prod",
    "setup-env-ios-dev": "npm run echo-env-dev; npm run cp-firebase-ios-dev",
    "setup-env-ios-prod": "npm run echo-env-prod; npm run cp-firebase-ios-prod",
    "cp-firebase-android-dev": "shx cp \"./app/config/files/config-dev/google-services.json\" \"./android/app/\"",
    "cp-firebase-android-prod": "shx cp \"./app/config/files/config-prod/google-services.json\" \"./android/app/\"",
    "cp-firebase-ios-dev": "shx cp \"./app/config/files/config-dev/GoogleService-Info.plist\" \"./ios/\"",
    "cp-firebase-ios-prod": "shx cp \"./app/config/files/config-prod/GoogleService-Info.plist\" \"./ios/\"",
    "echo-env-dev": "shx echo \"MEDIA_TRACKER_APP_ENV=dev\" > .env",
    "echo-env-prod": "shx echo \"MEDIA_TRACKER_APP_ENV=prod\" > .env",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "ios-clean": "cd ios && xcodebuild clean && cd ..",
    "ios-pod": "cd ios; pod install --repo-update; cd ..",
    "reset": "react-native start --reset-cache",
    "lint": "eslint 'app/**/*.ts' 'app/**/*.tsx'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.15.5",
    "@react-native-community/datetimepicker": "~3.5.2",
    "@react-native-community/masked-view": "~0.1.11",
    "@react-native-firebase/app": "~12.4.0",
    "@react-native-firebase/auth": "~12.4.0",
    "@react-native-picker/picker": "~1.16.5",
    "@react-navigation/drawer": "~6.1.1",
    "@react-navigation/native": "~6.0.2",
    "@react-navigation/stack": "~6.0.4",
    "axios": "~0.21.1",
    "class-transformer": "~0.4.0",
    "class-transformer-validator": "~0.9.1",
    "class-validator": "~0.13.1",
    "date-fns": "~2.23.0",
    "formik": "~2.2.9",
    "i18n-js": "~3.8.0",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-config": "~1.4.3",
    "react-native-document-picker": "~6.0.1",
    "react-native-flash-message": "~0.1.23",
    "react-native-fs": "~2.18.0",
    "react-native-gesture-handler": "~1.10.3",
    "react-native-reanimated": "~1.13.3",
    "react-native-safe-area-context": "~3.3.0",
    "react-native-screens": "~3.5.0",
    "react-redux": "~7.2.4",
    "redux": "~4.1.1",
    "redux-saga": "~1.1.3",
    "string-strip-html": "~8.3.0",
    "yup": "~0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "~7.14.5",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/i18n-js": "~3.8.2",
    "@types/jest": "^26.0.23",
    "@types/react": "~17.0.16",
    "@types/react-native": "^0.64.5",
    "@types/react-redux": "~7.1.18",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux": "~3.6.31",
    "@types/string-strip-html": "~5.0.1",
    "@typescript-eslint/eslint-plugin": "~4.29.1",
    "@typescript-eslint/parser": "~4.29.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "~2.24.0",
    "eslint-plugin-jsdoc": "~36.0.6",
    "eslint-plugin-react": "~7.24.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "17.0.1",
    "reflect-metadata": "~0.1.13",
    "shx": "~0.3.3",
    "typescript": "~4.3.5"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}

{
  "name": "react-media-tracker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android-dev": "SET ENVFILE=.env && npm run cp-firebase-android-dev && react-native run-android",
    "android-prod": "SET ENVFILE=.env.production && npm run cp-firebase-android-prod && react-native run-android --variant=release",
    "ios-dev": "ENVFILE=.env; npm run cp-firebase-ios-dev; react-native run-ios",
    "ios-dev-ipad": "ENVFILE=.env; npm run cp-firebase-ios-dev; react-native run-ios --simulator \"iPad (7th generation)\"",
    "ios-prod": "ENVFILE=.env.production; npm run cp-firebase-ios-prod; react-native run-ios --device --configuration Release",
    "cp-firebase-android-dev": "shx cp \"./app/config/files/config-dev/google-services.json\" \"./android/app/\"",
    "cp-firebase-android-prod": "shx cp \"./app/config/files/config-prod/google-services.json\" \"./android/app/\"",
    "cp-firebase-ios-dev": "shx cp \"./app/config/files/config-dev/GoogleService-Info.plist\" \"./ios/\"",
    "cp-firebase-ios-prod": "shx cp \"./app/config/files/config-prod/GoogleService-Info.plist\" \"./ios/\"",
    "android-clean": "cd android && \"./gradlew\" clean && cd ..",
    "ios-pod": "cd ios; pod install --repo-update; cd ..",
    "reset": "react-native start --reset-cache",
    "lint": "eslint app/**/*.ts app/**/*.tsx"
  },
  "dependencies": {
    "@react-native-community/async-storage": "~1.11.0",
    "@react-native-community/datetimepicker": "~2.4.0",
    "@react-native-community/masked-view": "~0.1.10",
    "@react-native-community/picker": "~1.6.1",
    "@react-native-firebase/app": "~7.1.4",
    "@react-native-firebase/auth": "~8.0.2",
    "axios": "~0.19.2",
    "class-transformer": "~0.2.3",
    "class-transformer-validator": "~0.8.0",
    "class-validator": "~0.12.2",
    "date-fns": "~2.14.0",
    "formik": "~2.1.4",
    "i18n-js": "~3.7.0",
    "react": "~16.13.1",
    "react-native": "~0.62.2",
    "react-native-config": "~1.2.0",
    "react-native-document-picker": "~3.4.0",
    "react-native-flash-message": "~0.1.15",
    "react-native-fs": "~2.16.6",
    "react-native-gesture-handler": "~1.6.1",
    "react-native-reanimated": "~1.9.0",
    "react-native-safe-area-context": "~3.0.2",
    "react-native-screens": "~2.8.0",
    "react-navigation": "~4.3.9",
    "react-navigation-drawer": "~2.4.13",
    "react-navigation-stack": "~2.5.1",
    "react-redux": "~7.2.0",
    "redux": "~4.0.5",
    "redux-saga": "~1.1.3",
    "string-strip-html": "~4.4.7",
    "yup": "~0.29.1"
  },
  "devDependencies": {
    "@babel/core": "~7.10.2",
    "@babel/plugin-proposal-decorators": "~7.10.1",
    "@babel/runtime": "~7.10.2",
    "@react-native-community/eslint-config": "1.1.0",
    "@types/i18n-js": "~3.0.3",
    "@types/jest": "~25.2.3",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.62.12",
    "@types/react-redux": "~7.1.9",
    "@types/react-test-renderer": "~16.9.2",
    "@types/redux": "~3.6.0",
    "@types/string-strip-html": "~4.3.0",
    "@types/yup": "~0.29.1",
    "@typescript-eslint/eslint-plugin": "~3.0.2",
    "@typescript-eslint/parser": "~3.0.2",
    "babel-jest": "~26.0.1",
    "eslint": "~7.1.0",
    "eslint-plugin-import": "~2.20.2",
    "eslint-plugin-jsdoc": "~26.0.1",
    "eslint-plugin-react": "~7.20.0",
    "jest": "~26.0.1",
    "metro-react-native-babel-preset": "~0.59.0",
    "react-test-renderer": "~16.13.1",
    "reflect-metadata": "~0.1.13",
    "shx": "~0.3.2",
    "typescript": "~3.9.3"
  },
  "jest": {
    "preset": "react-native"
  }
}

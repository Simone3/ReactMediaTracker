{
  "name": "react-media-tracker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "-------------------- comment 1 --------------------": "Main scripts:",
    "android-dev": "npm run setup-env-android-dev && react-native run-android",
    "android-prod": "npm run setup-env-android-prod && react-native run-android --variant=release",
    "ios-dev": "npm run setup-env-ios-dev; react-native run-ios",
    "ios-dev-ipad": "npm run setup-env-ios-dev; react-native run-ios --simulator \"iPad (7th generation)\"",
    "ios-prod": "npm run setup-env-ios-prod; react-native run-ios --device --configuration Release",
    "-------------------- comment 2 --------------------": "Util scripts:",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "ios-clean": "cd ios && xcodebuild clean && cd ..",
    "ios-pod": "cd ios; pod install --repo-update; cd ..",
    "reset": "react-native start --reset-cache",
    "lint": "eslint 'app/**/*.ts' 'app/**/*.tsx'",
    "-------------------- comment 3 --------------------": "Script components:",
    "setup-env-android-dev": "npm run echo-env-dev && npm run cp-firebase-android-dev",
    "setup-env-android-prod": "npm run echo-env-prod && npm run cp-firebase-android-prod",
    "setup-env-ios-dev": "npm run echo-env-dev; npm run cp-firebase-ios-dev",
    "setup-env-ios-prod": "npm run echo-env-prod; npm run cp-firebase-ios-prod",
    "cp-firebase-android-dev": "shx cp \"./app/config/files/config-dev/google-services.json\" \"./android/app/\"",
    "cp-firebase-android-prod": "shx cp \"./app/config/files/config-prod/google-services.json\" \"./android/app/\"",
    "cp-firebase-ios-dev": "shx cp \"./app/config/files/config-dev/GoogleService-Info.plist\" \"./ios/\"",
    "cp-firebase-ios-prod": "shx cp \"./app/config/files/config-prod/GoogleService-Info.plist\" \"./ios/\"",
    "echo-env-dev": "shx echo \"MEDIA_TRACKER_APP_ENV=dev\" > .env",
    "echo-env-prod": "shx echo \"MEDIA_TRACKER_APP_ENV=prod\" > .env"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.7",
    "@react-native-community/datetimepicker": "6.3.1",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-firebase/app": "15.2.0",
    "@react-native-firebase/auth": "15.2.0",
    "@react-native-picker/picker": "2.4.3",
    "@react-navigation/drawer": "6.4.3",
    "@react-navigation/native": "6.0.11",
    "@react-navigation/stack": "6.2.2",
    "axios": "0.27.2",
    "class-transformer": "0.5.1",
    "class-transformer-validator": "0.9.1",
    "class-validator": "0.13.2",
    "date-fns": "2.29.1",
    "formik": "2.2.9",
    "i18n-js": "4.0.2",
    "react": "18.0.0",
    "react-native": "0.69.3",
    "react-native-config": "1.4.6",
    "react-native-document-picker": "8.1.1",
    "react-native-flash-message": "0.3.1",
    "react-native-fs": "2.20.0",
    "react-native-gesture-handler": "2.5.0",
    "react-native-reanimated": "2.9.1",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "3.15.0",
    "react-redux": "8.0.2",
    "redux": "4.2.0",
    "redux-saga": "1.1.3",
    "string-strip-html": "8.3.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@babel/plugin-proposal-decorators": "7.18.10",
    "@babel/runtime": "7.18.9",
    "@react-native-community/eslint-config": "3.1.0",
    "@tsconfig/react-native": "2.0.2",
    "@types/i18n-js": "3.8.2",
    "@types/jest": "28.1.6",
    "@types/react": "18.0.15",
    "@types/react-native": "0.69.3",
    "@types/react-redux": "7.1.24",
    "@types/react-test-renderer": "18.0.0",
    "@types/redux": "3.6.31",
    "@types/string-strip-html": "5.0.1",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "babel-jest": "28.1.3",
    "eslint": "8.21.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.4",
    "eslint-plugin-react": "7.30.1",
    "jest": "28.1.3",
    "metro-react-native-babel-preset": "0.72.0",
    "react-test-renderer": "18.0.0",
    "reflect-metadata": "0.1.13",
    "shx": "0.3.4",
    "typescript": "4.7.4"
  },
  "resolutions": {
    "@types/react": "17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}

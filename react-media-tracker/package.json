{
  "name": "react-media-tracker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "-------------------- comment 1 --------------------": "Main scripts:",
    "android-dev": "npm run setup-env-android-dev && react-native run-android",
    "android-prod": "npm run setup-env-android-prod && react-native run-android --mode=release",
    "ios-dev": "npm run setup-env-ios-dev; react-native run-ios",
    "ios-dev-ipad": "npm run setup-env-ios-dev; react-native run-ios --simulator \"iPad (7th generation)\"",
    "ios-prod": "npm run setup-env-ios-prod; react-native run-ios --device --mode Release",
    "-------------------- comment 2 --------------------": "Util scripts:",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "ios-clean": "cd ios && xcodebuild clean && cd ..",
    "ios-pod": "cd ios; pod install --repo-update; cd ..",
    "reset": "react-native start --reset-cache",
    "lint": "eslint 'app/**/*.ts' 'app/**/*.tsx'",
    "-------------------- comment 3 --------------------": "Script components:",
    "setup-env-android-dev": "npm run echo-env-dev && npm run cp-firebase-android-dev",
    "setup-env-android-prod": "npm run echo-env-prod && npm run cp-firebase-android-prod",
    "setup-env-ios-dev": "npm run echo-env-dev; npm run cp-firebase-ios-dev",
    "setup-env-ios-prod": "npm run echo-env-prod; npm run cp-firebase-ios-prod",
    "cp-firebase-android-dev": "shx cp \"./app/config/files/config-dev/google-services.json\" \"./android/app/\"",
    "cp-firebase-android-prod": "shx cp \"./app/config/files/config-prod/google-services.json\" \"./android/app/\"",
    "cp-firebase-ios-dev": "shx cp \"./app/config/files/config-dev/GoogleService-Info.plist\" \"./ios/\"",
    "cp-firebase-ios-prod": "shx cp \"./app/config/files/config-prod/GoogleService-Info.plist\" \"./ios/\"",
    "echo-env-dev": "shx echo \"MEDIA_TRACKER_APP_ENV=dev\" > .env",
    "echo-env-prod": "shx echo \"MEDIA_TRACKER_APP_ENV=prod\" > .env"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.0.0",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-firebase/app": "20.5.0",
    "@react-native-firebase/auth": "20.5.0",
    "@react-native-picker/picker": "2.8.0",
    "@react-navigation/drawer": "6.7.2",
    "@react-navigation/native": "6.1.18",
    "@react-navigation/stack": "6.4.1",
    "@reduxjs/toolkit": "2.2.7",
    "axios": "1.7.7",
    "class-transformer": "0.5.1",
    "class-transformer-validator": "0.9.1",
    "class-validator": "0.14.1",
    "date-fns": "3.6.0",
    "formik": "2.4.6",
    "i18n-js": "4.4.3",
    "react": "18.3.1",
    "react-native": "0.75.3",
    "react-native-config": "1.5.3",
    "react-native-document-picker": "9.3.1",
    "react-native-flash-message": "0.4.2",
    "react-native-fs": "2.20.0",
    "react-native-gesture-handler": "2.19.0",
    "react-native-reanimated": "3.15.2",
    "react-native-safe-area-context": "4.11.0",
    "react-native-screens": "3.34.0",
    "react-redux": "9.1.2",
    "redux-saga": "1.3.0",
    "string-strip-html": "8.3.0",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/plugin-proposal-decorators": "7.24.7",
    "@babel/preset-env": "7.20.2",
    "@babel/runtime": "7.20.13",
    "@eslint/js": "9.10.0",
    "@react-native/babel-preset": "0.75.3",
    "@react-native/eslint-config": "0.75.3",
    "@react-native/metro-config": "0.75.3",
    "@react-native/typescript-config": "0.75.3",
    "@types/eslint__js": "8.42.3",
    "@types/i18n-js": "3.8.9",
    "@types/react": "18.2.79",
    "@types/react-test-renderer": "18.0.0",
    "@types/redux": "3.6.31",
    "@types/string-strip-html": "5.0.1",
    "@typescript-eslint/eslint-plugin": "8.5.0",
    "babel-jest": "29.6.3",
    "babel-plugin-module-resolver": "5.0.2",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-jsdoc": "50.2.3",
    "jest": "29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "reflect-metadata": "0.2.2",
    "shx": "0.3.4",
    "typescript": "5.0.4",
    "typescript-eslint": "8.5.0"
  },
  "engines": {
    "node": ">=18"
  }
}
